<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation=
    "urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Linh_Hotel::config_hotel" title="Hotel"></resource>
                        </resource>
                    </resource>
                </resource>



                <resource id="Linh_Hotel::management" title="Management" sortOrder="50">
                    <resource id="Linh_Hotel::booking" title="Booking Management" sortOrder="50">
                        <resource id="Linh_Hotel::bookingection"
                                  title="Booking Section" sortOrder="60">
                        </resource>
                    </resource>
                </resource>


            </resource>
        </resources>
    </acl>
</config>
